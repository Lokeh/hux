{
    "compilerOptions": {
        "allowJs": false,
        "target": "es6",
        "declaration": true,
        "module": "commonjs",
        "noImplicitAny": false,
        "noImplicitReturns": true,
        "noImplicitThis": true,
        "noUnusedLocals": true,
        "noUnusedParameters": true,
        "strictNullChecks": true
    },
    "exclude": [
        "node_modules",
        "examples"
    ]
}